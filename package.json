  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-derequire": "^0.9.4",
    "eslint": "^2.12.0",
    "eslint-config-streamroot": "^1.0.1",
    "uglify-js": "^2.6.2"
  },
  "browserify": {
    "transform": [
      "babelify"
    ],
